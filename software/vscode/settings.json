{
    "[julia]": {
        "editor.defaultFormatter": "julialang.language-julia"
    },
    "cSpell.ignorePaths": [
        "**/vault/journal/*",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "node_modules",
        "package-lock.json",
        "vscode-extension"
    ],
    "cSpell.userWords": [
        "codestyles",
        "intellij",
        "keymaps",
        "quicklists"
    ],
    "debug.inlineValues": true,
    "diffEditor.codeLens": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorWidth": 3,
    "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnPaste": true,
    "editor.inlineSuggest.enabled": true,
    "editor.letterSpacing": 0.4,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestions": true,
    "editor.scrollBeyondLastLine": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "first",
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.experimental.fileNesting.enabled": true,
    "files.associations": {
        "*.kws": "xml"
    },
    "files.autoSave": "afterDelay",
    "files.eol": "\n",
    "files.exclude": {
        "**/*.jar": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "nim": false,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.keymap": "none",
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "julia.additionalArgs": [],
    "julia.cellDelimiters": [
        "^##(?!#)",
        "^#(\\s?)%%",
        "^#-"
    ],
    "julia.enableTelemetry": false,
    "julia.environmentPath": "",
    "julia.execution.inlineResults.colors": {},
    "julia.runtimeCompletions": true,
    "julia.symbolCacheDownload": true,
    "redhat.telemetry.enabled": true,
    "settingsSync.keybindingsPerPlatform": false,
    "svelte.enable-ts-plugin": true,
    "sync.gist": "54c7cc41f7b5c4d5ed1653cf0945064a",
    "terminal.external.windowsExec": "C:\\Program Files\\Alacritty\\alacritty.exe",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "args": [],
            "icon": "terminal-cmd",
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ]
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "PowerShell": {
            "icon": "terminal-powershell",
            "source": "PowerShell"
        },
        "Windows PowerShell": {
            "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "typescript.check.npmIsInstalled": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
    "typescript.format.insertSpaceAfterTypeAssertion": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
    "typescript.tsserver.maxTsServerMemory": 0,
    "typescript.tsserver.useSyntaxServer": "always",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#E8BA36",
        "editorBracketHighlight.foreground2": "#54A857",
        "editorBracketHighlight.foreground3": "#82AAFF",
        "editorBracketHighlight.foreground4": "#5060BB",
        "editorBracketHighlight.foreground5": "#179387",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
    },
    "workbench.colorTheme": "Jewel",
    "workbench.editor.languageDetection": false,
    "workbench.enableExperiments": false,
    "workbench.list.openMode": "doubleClick",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "json",
        "jsonc",
        "latex",
        "markdown",
        "plaintext",
        "restructuredtext",
        // "text",
        "yaml",
        "yml"
    ]
}
