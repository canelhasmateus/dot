{
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    },
    "[python]": {},
    "debug.inlineValues": "auto",
    "diffEditor.codeLens": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorWidth": 3,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.fontFamily": "'JetBrainsMono NF', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": true,
    "editor.letterSpacing": 0.4,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestions": {
        "comments": "on",
        "other": "on",
        "strings": "on"
    },
    "editor.scrollBeyondLastLine": false,
    "editor.semanticHighlighting.enabled": "configuredByTheme",
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "first",
    "emojisense.languages": {
        ".mmd": true,
        "git-commit": true,
        "markdown": true,
        "plaintext": {
            "emojiDecoratorsEnabled": false,
            "markupCompletionsEnabled": false
        },
        "scminput": true
    },
    "errorLens.enabledDiagnosticLevels": [
        "warning",
        "info",
        "error"
    ],
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.kws": "xml",
        "*.ts": "typescript"
    },
    "files.autoSave": "afterDelay",
    "files.eol": "\n",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.tmp.drivedownload": true,
        "**/.tmp.driveupload": true,
        "**/*.jar": true
    },
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "foam.files.attachmentExtensions": "pdf mp3 webm wav m4a mp4 avi mov rtf txt doc docx pages xls xlsx numbers ppt pptm pptx sh py js sql java kt",
    "foam.graph.titleMaxLength": 0,
    "gitlens.keymap": "none",
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "markdown-preview-enhanced.mermaidTheme": "dark",
    "markdown-preview-enhanced.previewTheme": "monokai.css",
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serifr",
    "markdown.preview.openMarkdownLinks": "inEditor",
    "markdownlint.config": {
        "code-block-style": {
            "style": "fenced"
        }
    },
    "markdownlint.run": "onSave",
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.keybindingsPerPlatform": false,
    "svelte.enable-ts-plugin": true,
    "svelte.plugin.svelte.note-new-transformation": false,
    "terminal.external.windowsExec": "C:\\Program Files\\Alacritty\\alacritty.exe",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.defaultProfile.windows": "pwsh",
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "args": [],
            "icon": "terminal-cmd",
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ]
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "PowerShell": {
            "icon": "terminal-powershell",
            "source": "PowerShell"
        },
        "pwsh": {
            "args": [
                "-nologo"
            ],
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
        },
        "Windows PowerShell": {
            "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "typescript.check.npmIsInstalled": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
    "typescript.format.insertSpaceAfterTypeAssertion": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
    "typescript.tsserver.maxTsServerMemory": 0,
    "typescript.tsserver.useSyntaxServer": "always",
    "window.title": "${rootName}",
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#E8BA36",
        "editorBracketHighlight.foreground2": "#54A857",
        "editorBracketHighlight.foreground3": "#82AAFF",
        "editorBracketHighlight.foreground4": "#5060BB",
        "editorBracketHighlight.foreground5": "#179387",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
    },
    "workbench.colorTheme": "Jewel",
    "workbench.editor.languageDetection": false,
    "workbench.enableExperiments": false,
    "workbench.list.openMode": "doubleClick",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "zenMode.hideActivityBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": true,
    "vim.camelCaseMotion.enable": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visualline": "block-outline",
    "vim.digraphs": {},
    "vim.easymotion": true,
    "vim.easymotionKeys": "asdfjklçzxcvmqweruiop",
    "vim.handleKeys": {},
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": false,
    "vim.insertModeKeyBindings": [],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "after": [],
            "before": [
                "o",
                "r"
            ],
            "commands": [
                {
                    "args": {},
                    "command": "extension.intellijRecentFiles",
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "r",
                "r"
            ],
            "commands": [
                {
                    "args": {},
                    "command": "editor.action.formatDocument",
                    "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus"
                }
            ]
        },
        {
            "after": [],
