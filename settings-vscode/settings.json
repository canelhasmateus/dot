{
    "debug.inlineValues": "auto",
    "diffEditor.codeLens": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorWidth": 3,
    "editor.fontFamily": "'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnPaste": true,
    "editor.inlineSuggest.enabled": true,
    "editor.letterSpacing": 0.4,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "editor.scrollBeyondLastLine": false,
    "editor.semanticHighlighting.enabled": "configuredByTheme",
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "first",
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.kws": "xml"
    },
    "files.autoSave": "afterDelay",
    "files.eol": "\n",
    "files.exclude": {
        "**/*.jar": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "nim": false,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.keymap": "none",
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "redhat.telemetry.enabled": true,
    "settingsSync.keybindingsPerPlatform": false,
    "svelte.enable-ts-plugin": true,
    "terminal.external.windowsExec": "C:\\Program Files\\Alacritty\\alacritty.exe",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "args": [],
            "icon": "terminal-cmd",
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ]
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "PowerShell": {
            "icon": "terminal-powershell",
            "source": "PowerShell"
        },
        "Windows PowerShell": {
            "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        },
        "pwsh": {
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
            "args": [
                "-nologo"
            ]
        }
    },
    "terminal.integrated.gpuAcceleration": "on",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#E8BA36",
        "editorBracketHighlight.foreground2": "#54A857",
        "editorBracketHighlight.foreground3": "#82AAFF",
        "editorBracketHighlight.foreground4": "#5060BB",
        "editorBracketHighlight.foreground5": "#179387",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
    },
    "workbench.colorTheme": "Jewel",
    "workbench.editor.languageDetection": false,
    "workbench.enableExperiments": false,
    "workbench.list.openMode": "doubleClick",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "editor.fontWeight": "1",
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "selection",
    "foam.graph.titleMaxLength": 0,
    "editor.largeFileOptimizations": false,
    "cSpell.enabled": false,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "markdownlint.config": {
        "code-block-style": {
            "style": "fenced"
        },
    },
    "markdownlint.run": "onSave",
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "window.title": "${rootName}",
    "workbench.activityBar.iconClickBehavior": "focus",
    "zenMode.hideActivityBar": false,
    "editor.columnSelection": true,
    "vim.camelCaseMotion.enable": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visualline": "block-outline",
    "vim.digraphs": {},
    "vim.easymotion": true,
    "vim.easymotionKeys": "asdfjklçzxcvmqweruiop",
    "vim.handleKeys": {},
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": false,
    "vim.insertModeKeyBindings": [],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindings": [],
